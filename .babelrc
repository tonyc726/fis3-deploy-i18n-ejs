{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "node": "6"
        },
        "modules": "cjs"
      }
    ]
  ],
  "plugins": [
    "lodash",
    [
      "@babel/plugin-transform-runtime",
      {
        "corejs": 3
      }
    ],
    "add-module-exports"
  ],
  "comments": false
}
